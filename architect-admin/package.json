{
    "name": "architect-admin",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@devexpress/dx-react-core": "^1.10.3",
        "@devexpress/dx-react-grid": "^1.10.3",
        "@devexpress/dx-react-grid-material-ui": "^1.10.3",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "@material-ui/lab": "^3.0.0-alpha.30",
        "@material-ui/styles": "^3.0.0-alpha.10",
        "axios": "^0.18.0",
        "classnames": "^2.2.6",
        "history": "^4.7.2",
        "immutable": "^4.0.0-rc.12",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "prop-types": "^15.7.2",
        "react": "^16.8.1",
        "react-dom": "^16.8.1",
        "react-redux": "^6.0.0",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^4.0.8",
        "react-scripts": "2.1.5",
        "recharts": "^1.5.0",
        "redux": "^4.0.1",
        "redux-form": "^8.1.0",
        "redux-immutable": "^4.0.0",
        "redux-saga": "^1.0.1",
        "reselect": "^4.0.0",
        "source-map-explorer": "^1.7.0"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "build-js": "react-scripts build",
        "dev": "REACT_APP_ENV=dev npm-run-all -p watch-css start-js",
        "uat": "REACT_APP_ENV=uat npm-run-all -p watch-css start-js",
        "build": "REACT_APP_ENV=production npm-run-all build-css build-js",
        "analyze": "source-map-explorer build/static/js/main.*",
        "eject": "react-scripts eject",
        "prettier": "find . \\( -name node_modules -o -name __generated__ \\) -prune -or \\( -name '*.js' -o -name '*.css' -o -name '*.scss' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "eslint": "5.12.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "prettier": "^1.16.4"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "tabWidth": 4,
        "printWidth": 160
    }
}
