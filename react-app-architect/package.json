{
    "name": "test-react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.18.0",
        "history": "^4.7.2",
        "immutable": "^3.8.2",
        "node-sass-chokidar": "^1.3.0",
        "npm-run-all": "^4.1.3",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "react-helmet": "^5.2.0",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^4.0.8",
        "redux": "^4.0.0",
        "redux-immutable": "^4.0.0",
        "redux-saga": "^0.16.0",
        "reselect": "^3.0.1",
        "socketcluster-client": "^13.0.0"
    },
    "devDependencies": {
        "react-scripts": "1.1.4"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/styles -o src/styles",
        "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles --watch --recursive",
        "start-js": "react-scripts start",
        "build-js": "react-scripts build",
        "start-dev": "REACT_APP_ENV=dev npm-run-all -p watch-css start-js",
        "start-uat": "REACT_APP_ENV=uat npm-run-all -p watch-css start-js",
        "build": "REACT_APP_ENV=production npm-run-all build-css build-js",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    }
}
